{
	"devDependencies": {
		"prisma": "^6.16.3"
	},
	"dependencies": {
		"@prisma/client": "^6.16.3",
		"ulid": "^3.0.1",
		"zod": "^4.1.11"
	},
	"prisma": {
		"schema": "backend/db/schema.prisma"
	},
	"scripts": {
    	"dev": "pnpm --filter frontend dev",
		"db:generate": "prisma generate --schema=backend/db/schema.prisma",
		"db:reset": "prisma migrate reset --schema=backend/db/schema.prisma",
		"db:migrate": "prisma migrate dev --schema=backend/db/schema.prisma"
	}
}
